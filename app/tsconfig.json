{
  "compilerOptions": {
      "outDir": "dist/",
      "rootDir": "ts/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es5",
      "lib": ["es2015", "es2017", "dom"],
      "jsx": "react",
      "keyofStringsOnly": true,
      "skipLibCheck": true
  },
  "exclude": [
      "node_modules",
      "dist"
  ]
}